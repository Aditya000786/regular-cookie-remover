{
    "manifest_version": 2,
    "name": "Regular Cookie Remover",
    "version": "0.0.1",

    "description": "Remove your cookies on a regular basis!",

    "background": {
        "scripts": ["src/main.js"]
    },

    "permissions": [
        "browsingData",
        "notifications",
        "storage"
    ]

  }
